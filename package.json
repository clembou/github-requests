    "body-parser": "^1.17.1",
    "cors": "^2.8.1",
    "dotenv": "^4.0.0",
    "express": "^4.15.2",
    "file-stream-rotator": "0.2.0",
    "foreman": "^2.0.0",
    "lodash": "^4.17.4",
    "morgan": "^1.8.1",
    "passport": "^0.4.0",
    "passport-azure-ad": "^3.0.5",
    "request": "^2.81.0",
    "secure-compare": "3.0.1"
  },
  "devDependencies": {
    "nodemon": "^1.11.0"
  }
